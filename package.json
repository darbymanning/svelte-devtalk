{
  "name": "svelte-devtalk",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p svelte-kit:dev typesafe-i18n codegen:watch",
    "svelte-kit:dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "yarn codegen --watch",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-graphql-request": "^4.3.4",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@neuekit/reboot": "^2.0.0-alpha.2",
    "@sveltejs/adapter-auto": "^1.0.0-next.17",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "axios": "^0.25.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.49.7",
    "stylelint": "^14.3.0",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.2",
    "typesafe-i18n": "^4.0.0"
  },
  "type": "module"
}
